let
    Origen = Table.FromRows(Json.Document(Binary.Decompress(Binary.FromText("", BinaryEncoding.Base64), Compression.Deflate)), let _t = ((type nullable text) meta [Serialized.Text = true]) in type table [English = _t, Spanish = _t]),
    #"Tipo cambiado" = Table.TransformColumnTypes(Origen,{{"English", type text}, {"Spanish", type text}})
in
    #"Tipo cambiado"
